{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "ignoreCommand": "[[ \"$VERCEL_GIT_COMMIT_REF\" != design-renewal/* ]]",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": false,
      "main_stg": false,
      "main_dev": false
    }
  }
}
